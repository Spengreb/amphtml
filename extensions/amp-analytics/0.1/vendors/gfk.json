{
  "requests": {
    "base": "https://${p}-config-preproduction.sensic.net/tp?p=${p}&m=${m}&t=${t}&sui=${sui}&r=${r}&ua=${ua}&ut=${ut}&pr=${pr}",
    "audio-play": "${base}&ty=PL&c=${c}&pr=${pageViewId64}",
    "audio-stop": "${base}&ty=ST",
    "heartbeat": "${base}&ty=HB"
  },
  "vars": {
    "sui": "${clientId(sui)}",
    "ty": "HB",
    "p": "au1",
    "c": "default",
    "t": "s2s-amp",
    "r": "${ampdocHost}",
    "ut": "${timestamp}",
    "ua": "${userAgent}"
  },
  "triggers": {
    "pulse": {
      "on": "timer",
      "timerSpec": {
        "interval": 10,
        "immediate": false,
        "stopSpec": {
          "on": "hidden"
        }
      },
      "request": "heartbeat"
    }
  },
  "transport": {
    "beacon": false,
    "xhrpost": false,
    "image": true
  }
}
